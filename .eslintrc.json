{
  "rules": {
    "strict": [2, "global"],
    "no-console": 0,
    "eqeqeq": 1,
    "no-extra-semi": 2,
    "indent": [1, 4, {"SwitchCase": 1}],
    "quotes": [0, "double"],
    "linebreak-style": [0, "unix"],
    "semi": [2, "always"],
    "strict":0
  },
  "env": {
    "node": true,
    "browser": false,
    "es6": true
  },
  "extends": "eslint:recommended"
}
