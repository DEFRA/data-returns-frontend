<notice version="2.3">
  <api-key>{{apiKey}}</api-key>
  <notifier>
    <name>Data-Returns Frontend</name>
    <version>{{appVersion}}</version>
    <url>{{appUrl}}</url>
  </notifier>
  <error>
    <class>{{errorClass}}</class>
    <message>{{errorMessage}}</message>
    <backtrace>
      <line method="{{method}}" file="{{fileName}}" number="{{lineNumber}}"/>
    </backtrace>
  </error>
  <request>
    <url>{{appUrl}}</url>
    <component/>
    <action/>
    <cgi-data>
      <var key="SERVER_NAME">{{serverName}}</var>
      <var key="HTTP_USER_AGENT">hapi.js</var>
    </cgi-data>
  </request>
  <server-environment>
    <project-root>{{projectRoot}}</project-root>
    <environment-name>{{environment}}</environment-name>
    <app-version>{{appVersion}}</app-version>
  </server-environment>
</notice>